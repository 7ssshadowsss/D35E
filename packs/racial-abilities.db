{"_id":"2lnXFQ59NO07sXyP","name":"Outburst","permission":{"default":0,"wbFPpmVfPG23MbMC":3},"type":"attack","data":{"description":{"value":"<p><span style=\"color: #191813; font-size: 13px;\">Once per day, for up to 4 rounds, a maenad can subjugate her mentality to gain a boost of raw physical power. When she does so, she takes a &ndash;2 penalty to Intelligence and Wisdom but gains a +2 bonus to Strength.</span></p>","chat":"","unidentified":""},"tags":[["Maenad"]],"activation":{"cost":1,"type":"swift"},"duration":{"value":"4","units":"round"},"target":{"value":""},"range":{"value":null,"units":"","long":null},"uses":{"value":1,"max":1,"per":"day","autoDeductCharges":true,"maxFormula":"1","allowMultipleUses":false,"chargesPerUse":null},"measureTemplate":{"type":"","size":0,"overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"actionType":"special","attackBonus":"","critConfirmBonus":"","damage":{"parts":[]},"attackParts":[],"formula":"","ability":{"attack":null,"damage":"","damageMult":1,"critRange":20,"critMult":2},"save":{"dc":"0","description":""},"effectNotes":"","attackNotes":"","attackType":"racialAbility","masterwork":false,"enh":null,"proficient":true,"primaryAttack":true,"showInQuickbar":false,"attack":{"parts":[]},"changes":[],"contextNotes":[],"identifiedName":"Outburst","specialActions":[{"name":"Outburst!","action":"Create unique \"Outburst\" from D35E.commonbuffs on self;Activate buff \"Outburst\" on self;","condition":"","img":"/systems/D35E/icons/buffs/outburst.png"}],"baseWeaponType":"","melded":false,"threatRangeExtended":false,"finesseable":false,"uniqueId":"rac-outburst","combatChanges":[],"activateActions":[],"deactivateActions":[],"container":"None","containerWeightless":false},"flags":{},"img":"systems/D35E/icons/classfeatures/outburst.png","effects":[]}
